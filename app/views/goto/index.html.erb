<h1>todo</h1>

<h2>Todo</h2>
<% @todos.each do |t|%>
<p>
<strong><%= t.name%></strong>
<small><%= link_to "mark as done",goto_path(t),:method =>:put%>
</p>
<%end%>
<h2>Todone</h2>
<% @todone.each do |t|%>
<p>
<strong><%= t.name%></strong>
<small><%= link_to "remove",t,:method =>:delete%>
</p>
<%end%>
<p><%= link_to "Add a New Post" , new_goto_path %></p>